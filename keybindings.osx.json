// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+shift+d",    "command": "cursorColumnSelectDown",
      "when": "textInputFocus" },
    { "key": "cmd+shift+r",    "command": "cursorColumnSelectUp",
        "when": "textInputFocus"
    },
    { "key": "option+cmd+up",           "command": "editor.action.moveLinesUpAction",
      "when": "editorTextFocus" },
    { "key": "option+cmd+down",         "command": "editor.action.moveLinesDownAction",
      "when": "editorTextFocus"
    },
    {
      "key": "option+down",
      "command": "editor.emmet.action.decrementNumberByOne"
    },
    {
      "key": "option+up",
      "command": "editor.emmet.action.incrementNumberByOne"
    },
    {
      "key": "cmd+t",
      "command": "editor.action.transpose"
    },
    {
      "key": "cmd+shift+/",
      "when": "editorTextFocus",
      "command": "editor.emmet.action.toggleComment"
    },
    {
      "key": "cmd+b",
      "command": "workbench.action.tasks.build"
    },
    {
      "key": "cmd+k cmd+o",
      "command": "workbench.action.terminal.focus"
    },
    {
      "key": "shift+cmd+g",
      "command": "editor.emmet.action.wrapWithAbbreviation",
      "when": "editorFocus"
    },
    { "key": "shift+enter",           "command": "-python.execSelectionInTerminal",
      "when": "editorFocus && !findInputFocussed && !python.datascience.hascodecells && !replaceInputFocussed && editorLangId == 'python'"
    },
    {
      "key": "cmd+.",
      "command": "editor.emmet.action.matchTag"
    },
    { "key": "cmd+y",           "command": "redo",
      "when": "textInputFocus && !editorReadonly"
    },
    { "key": "cmd+g",                "command": "workbench.action.gotoLine" },
  
  ]
